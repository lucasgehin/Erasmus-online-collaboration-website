<!DOCTYPE html>
<html ng-app>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/home.css' />
    
    
    <link rel="stylesheet" href="/javascripts/bootstrap/css/bootstrap.css" />
    <!--<link rel="stylesheet" href="/javascripts/bootstrap/css/bootstrap.min.css" />-->
    <link rel="stylesheet" href="/javascripts/bootstrap/css/bootstrap-responsive.css" />
    <!--<link rel="stylesheet" href="/javascripts/bootstrap/css/bootstrap-responsive.min.css" />-->


    <script type="text/javascript" src="/socket.io/socket.io.js"></script>


  </head>
  <body>
    

		    <div class="navbar navbar-fixed-top navbar-inverse">
		      <div class="navbar-inner">
		        <div class="container-fluid">		         
		          
		          <!-- <div class="nav-collapse"> -->
		            <ul class="nav">
		              <li>
		              	<button type="button" class="btn btn-success" >
		              		<span class="icon-pencil icon-white"></span>
		              		Documents
		              	</button>
		              </li>

		              <li>
		              	<button class="btn btn-success" >
		              		<span class="icon-facetime-video icon-white"></span>
		              		Chat
		              	</button>

		              </li>
		              <li>
		              	<button  class="btn btn-success">
		              		<span class="icon-calendar icon-white"></span>
		              		Calendar
		              	</button>
		              </li>

		            </ul>



		            <ul class="nav">
		              <li>
		              	<button type="button" class="btn btn-info" id="docs-link">
		              		<span class="icon-edit icon-white"></span>
		              		Pinned docs
		              	</button>
		              </li>

		              <li>
		              	<button class="btn btn-info" id="projects-link">
		              		<span class="icon-briefcase icon-white"></span>
		              		Projects
		              	</button>

		              </li>
		              <li>
		              	<button class="btn btn-info" id="users-link" >
		              		<span class="icon-user icon-white"></span>
		              		Users
		              	</button>
		              </li>

		            </ul>

		            <ul class="nav pull-right">
		            	<li class="dropdown">
		            		
			            	<div class="btn-group">
			            		<button type="button" class="btn btn-danger">Settings</button>
			            		<button type="button" class="btn btn-danger dropdown-toggle" data-toggle="dropdown">
			            			<span class="icon-arrow-down icon-white "></span>

			            			<span class="sr-only"></span>
			            		</button>
			            		<ul class="dropdown-menu">
			            			
			            			<li><a href="#">Help</a></li>
			            			<li><a href="#">Report a bug</a></li>
			            			<li  class="divider"></li>
			            			<li>
			            				<a href="#">
			            					<span class="logout-btn">
			            						<span class="icon-remove-sign"></span>
			            						Logout
			            					</span>
			            				</a>
			            			</li>
			            		</ul>
			            	</div>

		            	</li>
		            </ul>
		         <!--  </div> -->
		        </div>
		      </div>
		    </div>

  	<div class="main-container container-fluid border-bottom">
		    
		     <div class="row-fluid header-top">
		    	<div class="span6 separation-right">
		    		<div class="row-fluid ">
		    			<div class="span12 titre">
		    				What's new ?
		    			</div>
		    		</div>
		    		<div class="row-fluid ">
		    			<div class="span12 content">
		    				Content
		    			</div>
		    		</div>

		    	</div>
		    	<div class="span6">
		    		<div class="row-fluid ">
		    			<div class="span12 titre">
		    				Next events 
		    			</div>
		    		</div>
		    		<div class="row-fluid ">
		    			<div class="span12 content">
		    				Content
		    			</div>
		    		</div>

		    	</div>
		    	
		    
		    	
		    </div>
		    <div class="pinned-docs-section row-fluid header-section separation-top" >
		    	<div class="span12">
		    		<div class="row-fluid ">
		    			<div class="span12 titre">
		    				Pinned documents
		    			</div>
		    		</div>
		    		<div class="row-fluid ">
		    			<div class="span12 content">
		    				<%= lorem %>
		    			</div>
		    		</div>

		    	</div>	
		    </div>
		    <div class="projects-link-section row-fluid header-section separation-top">
		    	<div class="span12">
		    		<div class="row-fluid ">
		    			<div class="span12 titre">
		    				Projects
		    			</div>
		    		</div>
		    		<div class="row-fluid ">
		    			<div class="span12 content">
		    				<%= lorem %>
		    			</div>
		    		</div>

		    	</div>	
		    </div>
		    <div class="users-link-section row-fluid header-section separation-top">
		    	<div class="span12">
		    		
		    		<div class="row-fluid ">
		    			<div class="span12  users" ng-controller="User_Manager">
		    				<div class="navbar">
						      <div class="navbar-inner">
						        <div class="container">
						        	<div class="brand">Users</div>
						        </div>
						      </div>
						    </div>



						    <div class="content" >

						    	<span class="user" ng-repeat="u in list">
						    		{{u.username}}
						    	</span>
						    </div>
		    				
		    			</div>
		    		</div>

		    	</div>	
		    </div>
	</div>

    <!-- Scripts se chargants aprÃ¨s le document -->

    <script type="text/javascript" src="/javascripts/jquery-1.10.2.js"></script>
    <!--<script type="text/javascript" src="/javascripts/jquery-1.10.2.min.js"></script>-->

    <script type="text/javascript" src="/javascripts/home/jquery.flip.min.js"></script>
    <!--<script type="text/javascript" src="/javascripts/home/jquery.flip.js"></script>-->

    
    
    <script type="text/javascript" src="/javascripts/bootstrap/js/bootstrap.js"></script>
    <!--<script type="text/javascript" src="/javascripts/bootstrap/js/bootstrap.min.js"></script>-->

    <!--
    	<script type="text/javascript" src="/javascripts/jQueryRotateCompressed.2.2.js"></script>
	-->

    <script type="text/javascript" src="/javascripts/jquery-ui-1.10.3.custom.min-only-shake.js"></script>



    <!-- Chargement de nos scripts finaux-->

    <script type="text/javascript" src="/javascripts/home/main.js"></script>

    <script type="text/javascript" src="/javascripts/home/controller.js"></script>
    

    <script type="text/javascript" src="/javascripts/angular.js"></script>
    <!--<script type="text/javascript" src="/javascripts/angular.min.js"></script>-->
    


  </body>
</html>